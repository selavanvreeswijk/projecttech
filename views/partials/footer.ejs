            text-align: center;
        }

        ul{
            display: flex;
            flex-direction: row;
            justify-content: center;
            align-items: center;
            list-style: none;
            gap: 4em;
        }

        ul img{
            width: 2.5em;
            height: auto;
        }
    </style>
</head>
<body class="footer">

    <header>
        <nav>
            <ul>
